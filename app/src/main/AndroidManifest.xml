<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="spoticesar.uniftec.com.br.spoticesar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.BuscarAlbumActivity"
            android:label="@string/title_activity_buscar_album"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.BuscarArtistaActivity" />
        <activity android:name=".activity.BuscarMusicaActivity" />
        <activity android:name=".activity.DetalheMusicaActivity" />
        <activity android:name=".activity.DetalheArtistaActivity" />
        <activity android:name=".activity.DetalheAlbumActivity"></activity>
    </application>

</manifest>